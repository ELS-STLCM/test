    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.20/jquery-ui-1.8.20.custom.js?08262012")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.20/minified/jquery.dataTables-min.js?08262012")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/SMO/textAreaMaxlengthLimiter.js?08262012")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/SMO/competency.js?08262012")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/SMO/numericValidator.js?08262012")" type="text/javascript"></script>        
    <script src="@Url.Content("~/Scripts/SMO/folder.js?08262012")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/SMO/frontOffice.js?08262012")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/SMO/questionBank.js?08262012")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/SMO/patient.js?08262012")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/SMO/skillSet.js?08262012")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/SMO/simOfficeCommon.js?08262012")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/SMO/simOfficeConstants.js?08262012")" type="text/javascript"></script>
