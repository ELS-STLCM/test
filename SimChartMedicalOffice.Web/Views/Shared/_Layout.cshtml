    <script src="@Url.Content("~/Scripts/jquery.scrollTo-min.js?08262012")" type="text/javascript"></script> 
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/flexbox/jquery.flexbox.css?08262012")" media="print, screen" />
    <script type="text/javascript" src="@Url.Content("~/Scripts/flexbox/jquery.flexbox.js?08262012")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/flexbox/shCore.js?08262012")"></script>
    <script src="@Url.Content("~/Scripts/SMO/assignmentBuilder.js?08262012")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/FullCalendar/_loader.js?08292012")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/FullCalendar/jquery.qtip-1.0.0-rc3.js")" type="text/javascript"></script>    
    <script src="@Url.Content("~/Scripts/SMO/simOfficeCalendar.js")" type="text/javascript"></script> 
    <script src="@Url.Content("~/Scripts/SMO/appointment.js")" type="text/javascript"></script> 
    <script src="@Url.Content("~/Scripts/date.format.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.20/jquery.ui.draggable.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.20/jquery.ui.resizable.js")" type="text/javascript"></script>
</head>
<body>
    <div class="container_32">
        <div class="clear-height-spacing">
        </div>
        <div class="remove-div-space grid_32">
            <div id="header" class="header-back">
                @Html.Partial("_LogOnPartial")
            </div>
            <div class="remove-div-space" id="main" style="padding-top: 0px">
                @RenderBody()
                <div class="clear-height-spacing">
                </div>
            @if (ConfigurationManager.AppSettings["ENVIRONMENT"] == "OFFSHORE")
            {
                <a id="debug" onclick="showOrhideDebugDiv()" class="debug-font"><u>Debug</u></a>
                <a id="clearDebug" onclick="clearDebugDiv()" class="debug-font"><u>Clear</u></a>
                <div class="grid_32 hide-content div-with-border loading-time" id="loadTime">
                     <ul id="loaddata"></ul>
                </div>
            }
            </div>
            <div id="footer" class="remove-div-space" style="height: 15px">
                Copyright © 2014 <a href="http://www.elsevier.com/" class="link select-hand" target="_blank">
                    Elsevier</a> Inc. All Rights Reserved.</div>
        </div>
    </div>

<div id="heartBeat" style="display: none;">
        <iframe id="iheartBeat" frameborder="0" width="0%" height="0px" style="display: none;"
            src="../Account/HeartBeat"></iframe>
    </div>
    <script type="text/javascript">
        var start;
        var environmentVar = '@ConfigurationManager.AppSettings["ENVIRONMENT"]';
        $('#main').ajaxStart(function () {
            start = new Date().getTime();
        });
        $('#main').ajaxComplete(function (e, xhr, settings) {
            var end = new Date(e.timeStamp).getTime();
            var time = end - start;
            if (environmentVar == "OFFSHORE")
                $("#loaddata").append("<LI>" + settings.url.split('?')[0] + ":" + time + " ms</LI>");
        });
        function showOrhideDebugDiv() {
            if($("#loadTime").hasClass('hide-content')){
                $("#loadTime").removeClass('hide-content').addClass('show-content');
            } else{
                $("#loadTime").removeClass('show-content').addClass('hide-content');     
            }
        }
        function clearDebugDiv(){
            $("#loaddata").empty();
            $("#loadTime").removeClass('show-content').addClass('hide-content');
        }
        $(document).ready(function () {
            var windowHeight = $(window).height();
            var headerHeight = $('#header').innerHeight();
            var footerHeight = $('#footer').innerHeight();
            var clearSpace = 40;
            var appheight = windowHeight - headerHeight - footerHeight - clearSpace;
            $('#main').css('height', appheight);
        });
    </script>
</body>
</html>
