    <link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css?08262012")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/SMO/simOfficeOverride.css?08262012")" rel="stylesheet" type="text/css" />
    <link  rel="stylesheet" type="text/css" href="@Url.Content("~/Content/SMO/simOffice.css?08262012")"/>
    <script src="@Url.Content("~/Scripts/jquery-1.7.2.js?08262012")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.blockUI.js?08262012")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js?08262012")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.alerts.js?08262012")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js?08262012")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.watermark.js?08262012")" type="text/javascript"></script>
